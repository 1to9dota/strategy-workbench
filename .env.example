# OKX API（先用模拟盘 flag=1）
OKX_API_KEY=your_api_key
OKX_SECRET=your_secret
OKX_PASSPHRASE=your_passphrase
OKX_FLAG=1

# JWT 单用户认证（必须修改为强密码/密钥！）
AUTH_USERNAME=admin
AUTH_PASSWORD=        # 必填：设置一个强密码
JWT_SECRET=           # 必填：运行 python -c "import secrets; print(secrets.token_hex(32))" 生成

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_bot_token
TELEGRAM_CHAT_ID=your_chat_id

# 数据库路径
DB_PATH=data/workbench.db

# 服务端口
API_PORT=8000
